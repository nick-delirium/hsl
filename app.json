{
  "expo": {
    "name": "Hansang Lab",
    "slug": "HSL",
    "privacy": "public",
    "scheme": "hslapp",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcons/playstore_icon_HSL.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/images/appIcons/playstore_icon_HSL.png",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Hansang Lab",
      "color": "#000000"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "stas-t",
            "project": "stas-t",
            "authToken": "3278003f71294ce2b856b95cd73c46f89e664223a4234c7e8b3ffdec2fd7c763"
          }
        }
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "hansanglab.com",
      "icon": "./assets/images/appIcons/app_store_icon_HSL.png",
      "config": {
        "googleMapsApiKey": "key"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_CONTACTS",
        "VIBRATE",
        "com.anddoes.launcher.permission.UPDATE_COUNT",
        "com.google.android.c2dm.permission.RECEIVE",
        "com.google.android.providers.gsf.permission.READ_GSERVICES",
        "com.sec.android.provider.badge.permission.READ",
        "com.sec.android.provider.badge.permission.WRITE",
        "com.sonyericsson.home.permission.BROADCAST_BADGE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "versionCode": 21,
      "package": "com.hansanglab.hansanglabapp",
      "icon": "./assets/images/appIcons/playstore_icon_HSL.png",
      "config": {
        "googleMaps": {
          "apiKey": "key"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "hansanglab.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}
