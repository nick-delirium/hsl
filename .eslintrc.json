{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/prop-types": 0,
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "no-console": 0,
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "global-require": 0,
        "no-alert": 0,
        "no-nested-ternary": 0,
        "no-return-assign": 0,
        "no-mixed-operators": 0,
        "import/no-unresolved": 0,
        "react/no-string-refs": 0,
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"]
    }
}
